#ifndef IPRT_INCLUDED_nocrt_algorithm
#define IPRT_INCLUDED_nocrt_algorithm
#ifndef RT_WITHOUT_PRAGMA_ONCE
# pragma once
#endif

#ifdef _MSC_VER
# pragma warning(push)
# pragma warning(disable: 4643) /* warning C4643: Forward declaring 'ios_base' in namespace std is not permitted by the C++ Standard */
#endif

namespace std
{
    template<typename a_RandomIt>
    void sort(a_RandomIt a_ItBegin, a_RandomIt a_ItEnd);

    template<typename a_RandomIt, typename a_Compare>
    void sort(a_RandomIt a_ItBegin, a_RandomIt a_ItEnd, a_Compare);

}

#ifdef _MSC_VER
# pragma warning(pop)
#endif

#endif /* !IPRT_INCLUDED_nocrt_algorithm */


